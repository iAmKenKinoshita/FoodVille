{
  "name": "solo_mvp_project",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^2.3.0",
    "node-fetch": "^3.2.10",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start-back": "cd backend && nodemon server.js",
    "start-front": "react-scripts start",
    "migrate:latest": "npx knex migrate:latest --knexfile backend/db/knexfile",
		"migrate:down": "npx knex migrate:down --knexfile backend/db/knexfile",
		"seed:data": "npx knex seed:run --knexfile backend/db/knexfile",
    "build": "npm install && npm run migrate:latest && npm run seed:data && react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
